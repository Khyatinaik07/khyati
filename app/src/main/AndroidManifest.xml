<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.onlineserviceportal">

    <uses-feature
        android:glEsVersion="0x00020000"
        android:required="true" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission. ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="activity.basic.OnlineServiceApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_luncher_foreground"
        android:label="@string/app_name"
        android:logo="@mipmap/ic_luncher_foreground"
        android:roundIcon="@mipmap/ic_luncher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="${usesCleartextTraffic}"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name="activity.home.servicepackagelayout1.ServicePackageLayoutOneActivity"
            android:label="@string/title_activity_service_package_layout2"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name="activity.home.servicepackagelayout2.specificationdetail.SpecificationDetailActivity"
            android:label="@string/title_activity_specification_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="activity.home.changePassword.ChangePasswordActivity"
            android:label="@string/title_activity_change_password"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="activity.authentication.otpverify.VerifyOTPActivity"
            android:label="@string/title_activity_verify_otp"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="activity.authentication.forgotpassword.ForgotPasswordActivity"
            android:label="@string/title_activity_forgot_password"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="activity.home.MembershipActivity"
            android:label="@string/title_activity_membership"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="activity.home.servicepackagelayout2.ServicePackageActivity"
            android:label="@string/title_activity_service_package"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="activity.home.preferedservice.PrefferedServiceActivity"
            android:label="@string/title_activity_preffered_service"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="activity.home.BookingDetailActivity"
            android:label="@string/title_activity_booking_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="fragment.BookingHistoryFragment"
            android:label="@string/title_activity_booking_history_fragment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="fragment.BookingTypeActivity"
            android:label="@string/title_activity_booking_type"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="activity.home.editprofile.EditProfileActivity"
            android:label="@string/title_activity_edit_profile"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="fragment.myprofile.MyProfileFragment"
            android:label="@string/title_activity_my_profile_fragment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="activity.home.DateTimeActivity"
            android:label="@string/title_activity_date_time"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="activity.home.LocationActivity"
            android:label="@string/title_activity_location"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="fragment.BookingFragment"
            android:label="@string/title_activity_booking_fragment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="activity.home.allsubservice.SubServiceAllActivity"
            android:label="@string/title_activity_sub_service_all"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="activity.home.breifservicesubtype.SubTypeServiceActivity"
            android:label="@string/title_activity_sub_type_service"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="fragment.homepage.HomePageFragment"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="activity.home.homepagetab.HomePageTabActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name="activity.authentication.registration.RegistrationActivity"
            android:label="@string/title_activity_registration"
            android:theme="@style/AppTheme.NoActionBar"
            android:windowSoftInputMode="adjustNothing" />
        <activity
            android:name="activity.authentication.login.LoginActivity"
            android:label="@string/title_activity_login"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name="activity.basic.BaseActivity" />
        <activity
            android:name="activity.basic.MainActivity"
            android:label="@string/app_name"
            android:launchMode="singleTask"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <service
            android:name="activity.home.GetAddressIntentService"
            android:exported="false" />

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />
    </application>

</manifest>